    "status": "bash scripts/check-status.sh",
    "compile": "hardhat compile",
    "test:contracts": "hardhat test",
    "deploy:local": "hardhat run scripts/deploy.cjs --network localhost",
    "deploy:somnia": "hardhat run scripts/deploy.cjs --network somniaTestnet",
    "verify:somnia": "hardhat run scripts/verify-deployment.cjs --network somniaTestnet",
    "node": "hardhat node",
    "clean:contracts": "hardhat clean",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio"
