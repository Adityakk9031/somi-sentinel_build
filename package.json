  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx src/backend/server.ts",
    "dev:agent": "tsx src/agent/index.ts",
    "dev:relayer": "tsx src/relayer/server.ts",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:agent\" \"npm run dev:relayer\"",
